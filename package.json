{
  "name": "sps",
  "version": "1.0.0",
  "description": "A simple product store API",
  "main": "index.js",
  "scripts": {
    "client-install": "cd client && yarn install",
    "client-build": "cd client && yarn run build",
    "server-install": "yarn install",
    "client": "cd client && yarn start",
    "server": "node server.js",
    "start": "yarn run client-build && node server.js",
    "dev": "yarn run client-build && nodemon server.js",
    "lint": "./node_modules/.bin/eslint"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "author": "Ashraf Yusuf",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "1.7.0",
